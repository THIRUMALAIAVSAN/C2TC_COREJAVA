
public class switchstatement {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a=10;
		switch(a) {
		case '1':
			System.out.println("One");
			break
		case '10':
			System.out.println("Ten");
			break;
		case '8':
			System.out.println("Eight");
			break;
		}

	}

}
